<Window x:Class="Calc.Menu1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calc"
        mc:Ignorable="d"
        Title="Menu1" Height="430" Width="630" Background="LightPink">
    <StackPanel Margin="25">
        <Menu>
            <MenuItem Header="File">
                <MenuItem Header="Patient Authorization" Click="MenuItem_Click"></MenuItem>
                <MenuItem Header="Eligible Benefits" Click="MenuItem_Click_1"></MenuItem>
                <MenuItem Header="Claim Submission" Click="MenuItem_Click_2"></MenuItem>
            </MenuItem>
            <MenuItem Header="Edit"></MenuItem>
            <MenuItem Header="View"></MenuItem>
            <MenuItem Header="Git"></MenuItem>
            <MenuItem Header="Project"></MenuItem>
            <MenuItem Header="Build"></MenuItem>
        </Menu>
        <Frame x:Name="framemain" Width="610" Height="420" Background="LightGray"></Frame>
    </StackPanel>
</Window>
